{
  "expo": {
    "name": "clube-de-viagens-stellaa",
    "slug": "clube-de-viagens-stellaa",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-stella.png",
      "resizeMode": "cover",
      "backgroundColor": "#ac092d"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA7buS3Lhe5HxHDPZFf9xGgWtzaxlsFT-g"
        }
      },
      "package": "com.priferreira.clubedeviagensstella"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
